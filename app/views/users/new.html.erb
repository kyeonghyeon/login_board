<h1>회원가입</h1>
<%= form_for @user do |f| %>
    ID: <%= f.text_field :identification %><br>
    Email: <%= f.email_field :email %><br>
    Password: <%= f.password_field :password %><br>
    Password Confirmation: <%= f.password_field :password_confirmation %><br>
    name: <%= f.text_field :name %><br>
    nickname: <%= f.text_field :nickname %><br>
    Birthday: <%= f.date_field :birthday %><br>
    <%= f.submit '회원가입' %>

<% end %>